{
  "name": "markdown-image-paste",
  "displayName": "markdown-image-paste",
  "description": "update tiny the picture, upload to github, and feedback you a link when paste a picture to markdown file ",
  "version": "0.0.1",
  "publisher": "leonzhang",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.markdownPasteImage"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.markdownPasteImage",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "tinify": "^1.6.0-beta.2",
    "tree-kill": "^1.2.0"
  }
}
